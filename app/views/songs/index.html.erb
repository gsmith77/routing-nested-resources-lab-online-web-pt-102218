<ul>
  <% @songs.each do |song| %>
    <li><%= display_artist(song) %> - <%= link_to song.title, song_path(song) %></li>
    <% if song.artist %>
      <li><%= link_to song.artist.name, artist_song_path(song.artist, song.id) %> </li>
      <li><%= link_to song.title, artist_songs_path(song.artist) %></li>
      <% end %>
  <% end %>
  </ul>
  <br>

<%= link_to "Add New Song", new_song_path %>